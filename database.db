CREATE DATABASE HospitalDb;
USE HospitalDb;

CREATE TABLE PatientDetail(
Sno INT PRIMARY KEY,
name VARCHAR(30) NOT NULL,
age INT NOT NULL,
sex CHAR(1) NOT NULL,
Weight INT NOT NULL,
Height INT NOT NULL,
Bgroup VARCHAR(2) NOT NULL,
Fname VARCHAR(30) NOT NULL,
Adddress VARCHAR(30) NOT NULL,
State VARCHAR(30) NOT NULL,
City VARCHAR(30) NOT NULL,
Pno VARCHAR(15) NOT NULL,
Emial VARCHAR(30) NOT NULL,
Symptoms VARCHAR(100) NOT NULL
);